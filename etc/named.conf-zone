zone "vmNet2013." IN {
	type master;
	file "vmNet2013.db";
	allow-update { key rndc-key; };
};

zone "2.168.192.in-addr.arpa" {
	type master;
	file "db.192.168.2";
	allow-update { key rndc-key; };
};

options {
	recursion	false;
	listen-on { 192.168.2.1; };
	allow-query { 127.0.0.1; 192.168.2.0/24; };
};
